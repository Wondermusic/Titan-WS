            <TimelineMarkerThickness>0</TimelineMarkerThickness>
          </GuideStyle>
          <ChannelListWidth>177</ChannelListWidth>
          <TimelineMultiplier>7</TimelineMultiplier>
          <ChannelStyle StyleId="GuideChannelStyle" />

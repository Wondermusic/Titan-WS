            <NoFocusBrush x:type="ImageBrush" Image="\Remote\left_nf.png" Stretch="Uniform" />
            <FocusBrush x:type="ImageBrush" Image="\Remote\left_f.png" Stretch="Uniform" />
            <EnableLabel>false</EnableLabel>
          </ButtonStyle>
          <LabelText>NewButton</LabelText>
          <Actions>
            <Action Action="CloseWindow" />
          </Actions>
        </Control>
      </GroupControls>

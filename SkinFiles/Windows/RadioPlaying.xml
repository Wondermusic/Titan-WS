        <FontType>Titan</FontType>
        <FocusFontBrush x:type="ColorBrush" Color="#FFFFFFFF" />
        <NoFocusFontBrush x:type="ColorBrush" Color="#FF00B7FF" />
      </ButtonStyle>
      <LabelText>NewButton</LabelText>
